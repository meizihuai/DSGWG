
@{
    Layout = "~/Views/Shared/_SunLayoutWithDiv.cshtml";
    ViewBag.smallTitle = "账号设置";
    ViewBag.haveAttentionSpan = false;
}
@section head{
    <style>
        .dcf-label {
            text-align: left;
        }
    </style>

}
<div class="layui-form">
    <div class="layui-form-item">
        <label class="layui-form-label">设备ID</label>
        <label class="layui-form-label dcf-label">123456</label>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label ">DSG ID</label>
        <label class="layui-form-label dcf-label">dongchaofandongchaofan</label>
    </div>
    <div class="layui-card-header" style="margin-left: 15px;">修改管理员密码</div>
    <div class="layui-form-item">
        <label class="layui-form-label">原登录密码</label>
        <div class="layui-input-inline" style="width: 150px;">
            <input type="password" name="OldPwd" placeholder="" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label">新登录密码</label>
        <div class="layui-input-inline" style="width: 150px;">
            <input type="password" name="NewpPwd" placeholder="" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label">确认密码</label>
        <div class="layui-input-inline" style="width: 150px;">
            <input type="password" name="ConfirmPwd" placeholder="" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item" style="margin-left: 30px;">
        <button class="layui-btn">保存</button>
    </div>
</div>
@section script{

}